/* -- [ #nav_main_aux_wrap ] -- */
#nav_main_aux_wrap { position: absolute; bottom: 0; right: 0; }
#nav_main,
#nav_aux { display: inline; float: left; margin-left: 5px; }
#nav_main { background: #355667;
border-top-left-radius: 3px; -moz-border-radius-topleft: 3px; -webkit-border-top-left-radius: 3px;
border-top-right-radius: 3px; -moz-border-radius-topright: 3px; -webkit-border-top-right-radius: 3px;
padding: 5px 5px 0 5px; }
#nav_main,
#nav_main ul,
#nav_main ul li,
#nav_main ul li a { display: block; float: left; }
#nav_main ul { /* Empty Ruleset */ }
#nav_main ul li { /* Empty Ruleset */ }
#nav_main ul li.tab { margin-right: 5px; }
body.home #nav_main .homepage a { border-top: 2px solid #02A5F9; line-height: 26px; }
#nav_main ul li.tab a { background: #fff;
border-top-left-radius: 3px; -moz-border-radius-topleft: 3px; -webkit-border-top-left-radius: 3px;
border-top-right-radius: 3px; -moz-border-radius-topright: 3px; -webkit-border-top-right-radius: 3px;
display: block; line-height: 28px; padding: 0 6px; }
/* -- [ #nav_main .search ] -- */
#nav_main .search { background: #fff;
border-top-left-radius: 3px; -moz-border-radius-topleft: 3px; -webkit-border-top-left-radius: 3px;
border-top-right-radius: 3px; -moz-border-radius-topright: 3px; -webkit-border-top-right-radius: 3px;
height: 28px; position: relative; width: 200px; }
#nav_main input.text { border: 1px solid #999999; font-size: 11px; height: 15px; padding: 0; width: 130px; }
#nav_main .field_wrap { position: absolute; top: 4px; left: 5px; background: #C0D6E2; display: block; padding: 2px; width: 132px; }
#nav_main input.submit { position: absolute; top: 7px; right: 4px; background: #fff; border: none; color: #27343C; font-size: 12px; font-weight: bold; margin: 0; padding: 0; width: 48px; }
/* -- [ #nav_aux ] -- */
#nav_aux { position: relative; top: 11px; right: 0; color: #fff; text-align: right; width: 180px; }
#nav_aux a { color: #fff; padding: 0 4px; }
#nav_main li.search:hover ul.options { display: block; }
#nav_main li.search ul.options { position: absolute; top: 28px; left: 5px; background: #c0d6e2; display: none; z-index: 300; }
#nav_main li.search ul.options li { border-bottom: 2px groove #d8f1ff; display: block; font-size: 12px; margin: 0; width: 100%; }
#nav_main li.search ul.options li .inner { float: left; padding: 5px; }
#nav_main li.search ul.options li:last-child { border-bottom: none; }
#nav_main li.search ul.options li a { border-radius: 8px; -moz-border-radius: 8px; -webkit-border-radius: 8px; display: block; font-weight: normal; padding: 4px 8px; width: 100%; }
#nav_main li.search ul.options li a.active { font-weight: bold; background: #025d8c; color: white; }

