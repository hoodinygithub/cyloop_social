<h3>Geo Info</h3>
<table cellpadding="0" cellspacing="0" width="600">
  <tr>
    <td width="250"><b>Country</b></td>
    <td><%= @geo[:current_country] %></td>
  </tr>
  <tr><td colspan="2"><img src="/images/spacer.gif" height="6" width="1"></td></tr>
  <tr>
    <td width="250"><b>Remote IP</b></td>
    <td><%= @geo[:remote_ip] %></td>
  </tr>
  <tr><td colspan="2"><img src="/images/spacer.gif" height="6" width="1"></td></tr>
</table>
<br />
<h3>HTTP Headers</h3>
<table cellpadding="0" cellspacing="0" width="600">
<% request.headers.sort.each do |item| %>
  <tr>
    <td width="250"><b><%= item[0] %></b></td>
    <td><%= item[1] %></td>
  </tr>
  <tr><td colspan="2"><img src="/images/spacer.gif" height="6" width="1"></td></tr>
<% end %>
</table>
<br />
<h3>Environment Variables</h3>
<table cellpadding="0" cellspacing="0" width="600">
<% ENV.sort.each do |item| %>
  <tr>
    <td width="250"><b><%= item[0] %></b></td>
    <td><%= item[1] %></td>
  </tr>
  <tr><td colspan="2"><img src="/images/spacer.gif" height="6" width="1"></td></tr>
<% end %>
</table>