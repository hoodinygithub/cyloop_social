<%= render :partial => 'station_details_form' %>
<%= four_thumbs_to(@station_obj, :class => 'left_side', :clearer => true, :type => :small) %>
<div class="album_text2">
  <b><%= @station_obj.playable.name %></b>
  <%= link_to(image_tag(market_image_path('share_button_small.png'), :title => t('actions.share'), :class => 'png_fix valign_middle'), '#', :onclick => "Base.stations.share(#{@station_obj.id}); return false;") %>
  <br/>
  <small><%= station_contains(@station_obj.playable) %></small>
</div><!--/end top_buttons -->

<div class="album_actions2">
  <%= blue_button(t("radio.create_new_station"), :id => "create_station_button", :class => "create_new_station") %><br/>
</div>

