<li id="my_station_item_<%= my_station_item.station.id %>">
  <div class="song_name_large">
    <a href="<%= radio_path(:station_id => my_station_item.station.id)%>" class="grey_button"><span><span><img src="images/icon_play_small.gif" alt="" /></span></span></a> &nbsp; &nbsp; <b id="my_station_name_<%= my_station_item.station.id%>"><%= link_to my_station_item.name, radio_path(:station_id => my_station_item.station.id) %></b>
  </div>
  <div class="includes"><%= station_contains(my_station_item, 3, false)%></div>
  <div class="large_bnt2">
    <%= link_to_function("<span><span class=\"blue\"><i>#{t('actions.edit').downcase}&nbsp;" + image_tag('edit_btn_grey.gif', :alt => t('actions.edit')) + "</i></span></span>", "Base.stations.launch_edit_layer(#{my_station_item.station.id})", :class => "grey_button" )%>

    <!--a href="#" class="grey_button edit_station"><span><span class="blue"><i>edit <img src="images/edit_btn_grey.gif" alt="" /></i></span></span></a-->
    <%= link_to_function("<span><span class=\"blue\">" + image_tag('close_red.gif', :alt => t('actions.delete')) + "</span></span>", "Base.stations.remove_from_layer(#{my_station_item.id})", :class => "grey_button" ) unless my_station_item == @station_obj.playable %>
    <!--a href="#" class="grey_button"><span><span class="blue"><img src="images/close_red.gif" alt="" /></span></span></a-->
  </div>
  <br class="clearer" />
</li>
