<% if !@collection.empty? %>
  <%= render :partial => 'item', :collection => @collection %>
  <% (6 - @collection.size).times do %>
    <div class="albums_item reviews_list">
      <%= image_tag "empty_img.gif" %>
    </div>
  <% end %>
<% else %>
  <%= render 'blank_slate' %>
<% end %>
<form name='station_form' action="">
  <input type="hidden" id="station_to_delete" value="" />
</form>
