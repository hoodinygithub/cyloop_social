<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>MSN Music</title>
    <%= render 'shared/meta' %>
    <%= render 'shared/javascripts' %>
  </head>
  <body>
    <div id="container">
      <div id="msn_header_container">
        <%= banner_ad("top_banner") %>
        <% if site_includes(:msnmx, :msnbr, :msnlatam, :msnlatino, :msnar, :msncaen, :msncafr) %>
          <%= render "3rd_party_branding/msn_header_#{site_code}" %>
        <% end %>
      </div>

      <div id="header">
        <div id="header_content">
          <a href="<%= msn_home_page_link_path %>"><img src="images/logo.gif" id="logo2" alt="" /></a>
          <%= render 'shared/main_links' %>
          <%= render 'shared/login_links' %>
          <%= render 'shared/search_form' %>
          <%= render 'shared/user_data' %>
          <div class="clearer"></div>
        </div>
      </div>

      <%= render 'shared/footer' %>
    </div><!--/end container -->
  </body>
</html>