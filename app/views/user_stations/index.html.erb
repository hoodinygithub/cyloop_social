<% content_for :title do %>
  <%= possessive :stations if profile_account.is_a? User %>  
  <%= t('artist.stations_featuring_artist', :artist_name => profile_account.name) if profile_account.is_a? Artist %>  

<% end %>

<% content_for :sorting do %>
  <%= sort_link_to :latest %>
  /
  <%= sort_link_to :top %>
  /
  <%= sort_link_to :alphabetical %>
<% end %>

<% content_for :list do %>
  <%= render 'list' %>
<% end %>

<%= render 'shared/list' %>
