<% content_for :javascript do %>
  var recommended_stations_queue = [<%= @recommended_stations_queue.collect(&:artist_id).join(", ") if @recommended_stations_queue %>];
<% end %>

<div id="internal_content">
    <div id="main_content">
        <%= render :partial => 'dashboards/left_column' %>
        <div id="center_column">
          <% if profile_account == current_user %>
            <h1><%= t('home.recommended_stations') %></h1>
            <%= render :partial => 'shared/recommended_stations' %>
          <% else %>
            <h1><%= t('home.user_latest_stations') %></h1>
            <%= render :partial => 'shared/user_latest_stations' %>
          <% end %>
          <%= render 'dashboards/network_activity' %>
          <%= render 'dashboards/comment_list' %>
        </div><!--/end center_column -->
        
        <%= render :partial => 'dashboards/right_column' %>
        
        <div class="clearer"></div>
    </div><!--/end main_content -->
</div><!--/end internal_content -->
