<div id="left_column">
  <%= render 'dashboards/account_info' unless profile_owner? %>

  <div class="black_boxes">
    <%= render 'dashboards/user_stations'   if profile_user?  %>
    <%= render 'dashboards/following_count' if profile_owner? %>
    <div class="clearer"></div>
  </div><!--/end black_boxes -->

  <% if profile_account.artist? %>
  <%= render 'dashboards/owner_buttons' if profile_account.has_station? %>
  <% elsif profile_account.user? %>
  <%= render 'dashboards/user_buttons' %>
  <% end %>
  <%= user_sidebar_links %>
</div><!--/end left_column -->
