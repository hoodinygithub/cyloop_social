<% @collection.each do |item| %>
  <% if item.is_a? Hash %>
    <% if current_user and current_user != item['account'] and params[:controller] == 'followers' %>
      <li timestamp="<%= item['timestamp'] %>">
        <%= render :partial => 'shared/network_collection_info', :locals => {:item => item} %>
      </li>
    <% end %>
  <% elsif item.is_a? User or item.is_a? Artist %>
    <% if current_user and current_user != item and params[:controller] == 'followers' %>
      <li timestamp="<%= item['timestamp'] %>">
      <%= render :partial => 'shared/network_collection_info', :locals => {:item => item} %>
      </li>
    <% end %>
  <% end %>
<% end %>
