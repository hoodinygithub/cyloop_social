<%= javascript_include_tag 'jquery-1.4.1.min' %>
<%= javascript_include_tag 'jquery.cycle.all.min' %>
<%= javascript_include_tag 'jquery.cycle.all.min' %>
<%= javascript_include_tag 'supersleight.plugin' %>
<%= javascript_include_tag 'jquery.metadata' %>
<%= javascript_include_tag 'jquery.autocomplete' %>
<%= javascript_include_tag 'jquery.validate.min' %>
<%= javascript_include_tag 'jquery.hotkeys.min' %>
<%= javascript_include_tag 'jquery.facebox' %>
<%= javascript_include_tag 'base' %>
<%= javascript_include_tag '/utils/locale.js' %>

<%= javascript_tag "var AUTH_TOKEN = #{form_authenticity_token.to_json};" if protect_against_forgery? %>

<script>
  <% if profile_account %>
    Base.account.slug = "<%= profile_account.slug %>";
    Base.account.type = "<%= profile_account.artist? ? "artist" : "user" %>";
  <% end %>

  <%= yield(:javascript) %>
</script>

